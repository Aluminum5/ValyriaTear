#: ../data/story/ep1/layna_village/tutorial_battle_dialogs.lua:98
#: ../data/story/ep1/layna_village/tutorial_shop_dialogs.lua:67
#: ../data/story/ep1/layna_village/tutorial_shop_dialogs.lua:94
#: ../data/story/ep1/mt_elbrus/mt_elbrus_cave1_script.lua:116
#: ../data/story/ep1/mt_elbrus/mt_elbrus_cave1_script.lua:131
