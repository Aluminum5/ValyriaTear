#: ../data/story/ep1/layna_village/layna_village_riverbank_at_night_script.lua:210
#: ../data/story/ep1/layna_village/layna_village_riverbank_at_night_script.lua:223
#: ../data/story/ep1/mt_elbrus/battle_with_dark_soldiers_script.lua:33
#: ../data/story/ep1/mt_elbrus/mt_elbrus_path3_script.lua:136
#: ../data/story/ep1/mt_elbrus/mt_elbrus_path3_script.lua:139
#: ../data/story/ep1/mt_elbrus/mt_elbrus_path3_script.lua:142
#: ../data/story/ep1/mt_elbrus/mt_elbrus_path4_script.lua:142
